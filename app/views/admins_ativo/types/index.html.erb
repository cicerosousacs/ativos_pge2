<div class="row ">
  <div class="col-lg-12">
    <h1>Tipos de Ativos</h1>
       

        
  </div>
</div>

<br>
<table class="table">
  <thead>
    <tr>
      <th>Nome</th>
      <th style="width: 10px">
                  <%= link_to new_admins_ativo_type_path do %>
              <i class="fa fa-plus"></i>
            <% end %> 
      </th>
    </tr>
  </thead>

<table class="table table-hover table-bordered">
  <tbody>
    <% @types.each do |type| %>
      <tr>
        <td><%= type.description %></td>
        <td style="width: 10px">
          <%= link_to edit_admins_ativo_type_path(type) do %>
            <i class="fa fa-edit"></i>
          <% end %>
        <td style="width: 10px">
          <%= link_to admins_ativo_type_path(type), method: :delete, data: {confirm: "Certeza?"} do %>
            <i class="fa fa-trash"></i>
          <% end %>  
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

  